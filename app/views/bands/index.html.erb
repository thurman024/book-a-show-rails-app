<h1>Bands Index</h1>
<div class="band-list col s4">

    <% @bands.each do |band| %>
      <h5> <%= link_to band.name, band_path(band) %> - <%= show_count_message(band.shows) %></h5>
      <a href="/bands/<%=band.id%>/bookings.json" class="js-all">See all shows</a>
      <br>
    <% end %>

</div>
<div class="shows-list right col s8">

</div>
<br>
<p><%= link_to "Add a Band", new_band_path %></p>
