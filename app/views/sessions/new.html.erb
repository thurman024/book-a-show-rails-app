<%= form_for @user, :url => url_for(:controller => 'sessions', :action => 'create') do |f| %>

  <div >
    <%= f.label :username %><br>
    <%= f.text_field :username %><br>

  </div>

  <div >
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>

  <div>
    <%= f.submit "Sign In" %>
  </div>
<% end %>

<p>Or, sign in with Google</p>
<%= link_to 'Log In with Google', '/auth/google_oauth2' %>
